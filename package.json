    "gittar": "^0.1.1",
    "mina-signer": "^3.0.2",
    "o1js": "^0.16.*",
    "opener": "^1.5.2",
    "ora": "^8.0.1",
    "semver": "^7.6.0",
    "shelljs": "^0.8.5",
    "table": "^6.8.1",
